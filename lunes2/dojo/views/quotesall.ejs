<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Quotes</title>
    </head>
    <body>
        <h1>
            Quotes
        </h1>
        <main>
            <div>
                <% for(const quote of user ) {%>
                    <h2>
                        User: <%= quote.firstName %> <%= quote.lastName %>
                    </h2>
                    <h2>
                        Id: <%= quote.id %>
                    </h2>
                    <p>
                        Quote: "<%= quote.quote %>"
                    </p>
                    <p>
                        <%= quote.created_at %>
                    </p>
                <% } %>
            </div>
        </main>
    </body>
</html>